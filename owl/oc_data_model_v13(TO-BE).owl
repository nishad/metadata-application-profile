                <owl:onClass rdf:resource="http://www.w3.org/ns/oa#Annotation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy rdf:resource="http://open.library.ubc.ca/terms#"/>
        <rdfs:label xml:lang="en">AnnotationContainer</rdfs:label>
        <skos:definition xml:lang="en">This locally-created container class brings together multiple annotations that apply to an item. It also provides access to the full text of an item.</skos:definition>
    </owl:Class>
    


