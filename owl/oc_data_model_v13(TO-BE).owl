        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/date"/>
        <rdfs:domain rdf:resource="http://open.library.ubc.ca/terms#SourceResource"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">Sort Date</rdfs:label>
        <skos:definition xml:lang="en">A Dublin Core Elements Property; A point or period of time associated with an event in the lifecycle of the resource.; Date may be used to express temporal information at any level of granularity. Recommended best practice is to use an encoding scheme, such as the W3CDTF profile of ISO 8601 [W3CDTF].</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/description -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/description">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/description"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open.library.ubc.ca/terms#SourceResource"/>
                    <rdf:Description rdf:about="http://purl.org/dc/dcmitype/Collection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">Collection Description (dcmitype:Collection)</rdfs:label>
        <rdfs:label xml:lang="en">Description (oc:SourceResource)</rdfs:label>
        <skos:definition xml:lang="en">A Dublin Core Terms Property; An account of the resource.; Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource. (oc:SourceResource)</skos:definition>
        <skos:definition xml:lang="en">Free-text account of aggregation, for example an abstract or content scope note. Provided via OC admin tool. (dcmitype:Collection)</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/identifier -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/identifier"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open.library.ubc.ca/terms#DataDescription"/>
                    <rdf:Description rdf:about="http://open.library.ubc.ca/terms#SourceResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">Catalogue Number (oc:DataDescription)</rdfs:label>
        <rdfs:label xml:lang="en">Identifier (oc:SourceResource)</rdfs:label>
        <skos:definition xml:lang="en">A Dublin Core Terms Property; An unambiguous reference to the resource within a given context.; Recommended best practice is to identify the resource by means of a string conforming to a formal identification system. Mapped to: Access Identifier, Digital Identifier, UBC Call Number.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/issued">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/date"/>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/date"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open.library.ubc.ca/terms#SourceResource"/>
                    <rdf:Description rdf:about="http://open.library.ubc.ca/terms#WebResource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">Date Available (oc:WebResource)</rdfs:label>
        <rdfs:label xml:lang="en">Date Issued (oc:SourceResource)</rdfs:label>
        <skos:definition xml:lang="en">A Dublin Core Terms Property; Date of formal issuance (e.g., publication) of the resource. (oc:SourceResource)</skos:definition>
        <skos:definition xml:lang="en">A Dublin Core Terms Property; Date of formal issuance (e.g., publication) of the web resource. (oc:WebResource)</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/tableOfContents -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/tableOfContents">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/description"/>
        <rdfs:domain rdf:resource="http://open.library.ubc.ca/terms#PublicationDescription"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">Contents</rdfs:label>
        <skos:definition xml:lang="en">A Dublin Core Terms Property; A list of subunits of the resource.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/title">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/title"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://open.library.ubc.ca/terms#SourceResource"/>
                    <rdf:Description rdf:about="http://purl.org/dc/dcmitype/Collection"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/dc/terms/"/>
        <rdfs:label xml:lang="en">Collection Title (dcmitype:Collection)</rdfs:label>
        <rdfs:label xml:lang="en">Title (oc:SourceResource)</rdfs:label>
        <skos:definition xml:lang="en">A Dublin Core Terms Property; The name given to the resource. (oc:SourceResource)</skos:definition>
        <skos:definition xml:lang="en">Name of the collection or aggregation. Provided via OC admin tool. (dcmitype:Collection)</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#bathymetry -->

    <owl:DatatypeProperty rdf:about="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#bathymetry">
        <rdfs:domain rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#DepthBehav"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#"/>
        <rdfs:label xml:lang="en">Depth of Water</rdfs:label>
        <skos:definition xml:lang="en">Fisheries Metadata Element Set (FiMES); Bathymetry is numeric depth information about the ranges in which an aquatic species is found.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#bottomType -->

    <owl:DatatypeProperty rdf:about="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#bottomType">
        <rdfs:domain rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#FishingGround"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#"/>
        <rdfs:label xml:lang="en">Bottom</rdfs:label>
        <skos:definition xml:lang="en">Fisheries Metadata Element Set (FiMES); &quot;Marine habitat: type of bottom substratum in which the fishery resource is distributed.&quot; (FIRMS Inventory Guidelines 2011).</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#depthZone -->

    <owl:DatatypeProperty rdf:about="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#depthZone">
        <rdfs:domain rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#FishingGround"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#"/>
        <rdfs:label xml:lang="en">Depth of Capture</rdfs:label>
        <skos:definition xml:lang="en">Fisheries Metadata Element Set (FiMES); &quot;Marine habitat/Fishing ground: Bathymetric depth range in which a fishery resource is distributed or in which a fishery is conducted&quot; (FIRMS Inventory Guidelines 2011).</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#horizontalDist -->

    <owl:DatatypeProperty rdf:about="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#horizontalDist">
        <rdfs:domain rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#HabitatBio"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#"/>
        <rdfs:label xml:lang="en">Distance Offshore</rdfs:label>
        <skos:definition xml:lang="en">Fisheries Metadata Element Set (FiMES); /&quot;Marine habitat: inshore to offshore range in which the fishery resource is distributed (e.g. littoral, neritic, oceanic ...)./&quot; (FIRMS Inventory Guidelines 2011)</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#waterAreaOverview -->

    <owl:DatatypeProperty rdf:about="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#waterAreaOverview">
        <rdfs:domain rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#AqRes"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#"/>
        <rdfs:label xml:lang="en">Locality</rdfs:label>
        <skos:definition xml:lang="en">Fisheries Metadata Element Set (FiMES); &quot;Description of the water area in which the aquatic resource is located, including environmental characteristics, jurisdictional limits, ...&quot; (FIGIS-FIRMS 2002 workshop). Water Area Overview might contain a reference list of areas in which a species or aquatic resource is found or employed.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#waterCondition -->

    <owl:DatatypeProperty rdf:about="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#waterCondition">
        <rdfs:domain rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#HabitatBio"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.fao.org/fi/figis/devcon/fimes/doc/index.html#"/>
        <rdfs:label xml:lang="en">Temperature</rdfs:label>
        <skos:definition xml:lang="en">Fisheries Metadata Element Set (FiMES); WaterCondition is information on species affinity related to water conditions or characteristics , e.g. oxygen , temperature etc.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#value -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#value">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/oa#TextualBody"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
        <rdfs:label xml:lang="en">Text</rdfs:label>
        <skos:definition xml:lang="en">Text of the annotation.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#lat -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#lat">
        <rdfs:domain rdf:resource="http://www.europeana.eu/schemas/edm/Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#"/>
        <rdfs:label xml:lang="en">Latitude</rdfs:label>
        <skos:definition xml:lang="en">Basic Geo (WGS84 Lat/Long) Property; Latitude (φ) - Specified in Decimal Degrees</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#long -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#long">
        <rdfs:domain rdf:resource="http://www.europeana.eu/schemas/edm/Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#"/>
        <rdfs:label xml:lang="en">Longitude</rdfs:label>
        <skos:definition xml:lang="en">Basic Geo (WGS84 Lat/Long) Property; Longitude (λ) - Specified in Decimal Degrees</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#note -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#note">
        <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core#"/>
        <rdfs:label xml:lang="en">Note</rdfs:label>
        <skos:definition xml:lang="en">A general note, for any purpose.</skos:definition>
        <skos:scopeNote xml:lang="en">This property may be used directly, or as a super-property for more specific note types.</skos:scopeNote>
    </owl:DatatypeProperty>
    


