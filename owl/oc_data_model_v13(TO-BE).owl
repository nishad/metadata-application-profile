    <!-- http://dp.la/info/developers/map/SourceResource -->

    <owl:Class rdf:about="http://dp.la/info/developers/map/SourceResource">
        <rdfs:subClassOf rdf:resource="http://www.europeana.eu/schemas/edm/ProvidedCHO"/>
        <rdfs:isDefinedBy rdf:resource="http://dp.la/info/developers/map/"/>
        <rdfs:label xml:lang="en">Source Resource</rdfs:label>
        <skos:definition xml:lang="en">This class is a subclass of &quot;edm:ProvidedCHO,&quot; which comprises the described resources (in EDM called &quot;cultural heritage objects&quot;) about which DPLA collects descriptions. It is here that attributes of the described resources are located, not the digital representations of them.</skos:definition>
    </owl:Class>
    


    <!-- http://open.library.ubc.ca/terms#AnnotationContainer -->

    <owl:Class rdf:about="http://open.library.ubc.ca/terms#AnnotationContainer">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://open.library.ubc.ca/terms#annotation"/>
